        },
        "textMode": "auto"
      },
      "pluginVersion": "8.5.27",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "count(custom_backup_info)",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Monitored Servers",
      "type": "stat"
    },
    {
      "id": 2,
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto",
        "thresholds": [
          {
            "color": "super-light-green",
            "value": null
          },
          {
            "color": "red",
            "value": 0.1
          }
        ],
        "valueMappings": []
      },
      "pluginVersion": "8.5.27",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "count(custom_backup_job_success == 0)",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Jobs Failing",
      "type": "stat"
    },
    {
      "id": 3,
      "gridPos": {
        "h": 5,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "textMode": "auto",
        "thresholds": [
          {
            "color": "super-light-green",
            "value": null
          },
          {
            "color": "red",
            "value": 0.1
          }
        ],
        "valueMappings": []
      },
      "pluginVersion": "8.5.27",
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${DS_PROMETHEUS}"
          },
          "expr": "count(time() - custom_backup_last_success_timestamp_seconds > 25 * 3600)",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "No Recent Backup (>25h)",
      "type": "stat"
    },
    {
      "id": 4,
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 5
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "auto",
            "displayMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "red",
                "value": null
              },
              {
                "color": "super-light-green",
                "value": 1
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Status"
            },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "0": {
                        "color": "red",
                        "text": "Failed"
                      },
                      "1": {
                        "color": "super-light-green",
                        "text": "OK"
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Last Success"
            },
            "properties": [
              {
                "id": "unit",
                "value": "datetimeFromNow"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Duration (s)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "s"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Size (Bytes)"
            },
            "properties": [
              {
                "id": "unit",
                "value": "bytes"
              }
            ]
          }
        ]
      },
      "title": "Backup Status per Server",
      "type": "table",
      "targets": [
        {
          "datasource": "${DS_PROMETHEUS}",
          "expr": "custom_backup_job_success{server_ip=~\"$server_ip\"}",
          "instant": true,
          "legendFormat": "{{server_ip}}",
          "refId": "A"
        },
        {
          "datasource": "${DS_PROMETHEUS}",
          "expr": "custom_backup_last_success_timestamp_seconds{server_ip=~\"$server_ip\"}",
          "instant": true,
          "legendFormat": "{{server_ip}}",
          "refId": "B"
        },
        {
          "datasource": "${DS_PROMETHEUS}",
          "expr": "custom_backup_job_duration_seconds{server_ip=~\"$server_ip\"}",
          "instant": true,
          "legendFormat": "{{server_ip}}",
          "refId": "C"
        },
        {
          "datasource": "${DS_PROMETHEUS}",
          "expr": "custom_backup_archive_size_bytes{server_ip=~\"$server_ip\"}",
          "instant": true,
          "legendFormat": "{{server_ip}}",
          "refId": "D"
        },
        {
          "datasource": "${DS_PROMETHEUS}",
          "expr": "custom_backup_info{server_ip=~\"$server_ip\"}",
          "instant": true,
          "legendFormat": "{{server_ip}}",
          "refId": "E"
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {
              "Time": 0,
              "Value #A": 2,
              "Value #B": 3,
              "Value #C": 4,
              "Value #D": 5,
              "Value #E": 6,
              "version": 7
            },
            "renameByName": {
              "Value": "Server IP",
              "Value #A": "Status",
              "Value #B": "Last Success",
              "Value #C": "Duration (s)",
              "Value #D": "Size (Bytes)",
              "version": "Script Version"
            }
          }
        }
      ]
    },
